-- TSB Features
TSBtab:CreateButton({
   Name = "Show death contour",
   Callback = function()
      local loadNotify = Rayfield:Notify({
         Title = "Loading",
         Content = "Initializing Show death contour...",
         Duration = math.huge,
         Image = 4483362458
      })
      
      local success, err = pcall(function()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/louismich4el/ItsLouisPlayz-Scripts/main/TSB%20Death%20Counter%20Identifier.lua", true))()
      end)
      
      loadNotify:Destroy()
      
      if success then
         Rayfield:Notify({
            Title = "Success",
            Content = "Show death contour loaded!",
            Duration = 3,
            Image = 4483362458
         })
      else
         Rayfield:Notify({
            Title = "Error",
            Content = "Failed: "..err,
            Duration = 5,
            Image = 4483362458
         })
      end
   end
})

-- Final initialization message
Rayfield:Notify({
   Title = "Bro_broa Hub Ready",
   Content = "All systems initialized successfully!",
   Duration = 5,
   Image = 4483362458
})
