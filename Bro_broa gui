local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local noclipConnection = nil
local currentNoclipState = false

-- Safe collision update with pcall
local function updateCollision(state)
    pcall(function()
        if not player or not player.Character then return end
        
        for _, part in ipairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                -- Backup original collision
                if not part:FindFirstChild("OriginalCollision") then
                    local backup = Instance.new("BoolValue")
                    backup.Name = "OriginalCollision"
                    backup.Value = part.CanCollide
                    backup.Parent = part
                end
                
                -- Apply new state
                part.CanCollide = not state
            end
        end
    end)
end

-- Protected noclip loop
local function startNoclip()
    pcall(function()
        if noclipConnection then 
            noclipConnection:Disconnect() 
        end
        
        noclipConnection = RunService.Stepped:Connect(function()
            if currentNoclipState then
                updateCollision(true)
            end
        end)
    end)
end

-- Create protected toggle
local success, errorMsg = pcall(function()
    local NoclipToggle = PlayerTab:CreateToggle({
        Name = "Noclip",
        CurrentValue = false,
        Callback = function(value)
            pcall(function()
                currentNoclipState = value
                if value then
                    startNoclip()
                    updateCollision(true)
                else
                    if noclipConnection then
                        noclipConnection:Disconnect()
                    end
                    updateCollision(false)
                end
            end)
        end
    })
end)

if not success then
    warn("Noclip toggle creation failed:", errorMsg)
end

-- Auto-reconnect with protection
player.CharacterAdded:Connect(function()
    pcall(function()
        if currentNoclipState then
            task.wait(1) -- Wait for character load
            updateCollision(true)
            startNoclip()
        end
    end)
end)

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")

-- Enhanced Walk Speed Controller
local WalkSpeedController = {
    currentSpeed = nil,        -- Currently applied speed
    gameDefaultSpeed = nil,    -- Game's native walk speed
    usingCustomSpeed = false,  -- Whether we're overriding default
    lastSpeedCheck = 0,        -- Last time we checked default speed
    checkInterval = 1,         -- How often to verify default speed (seconds)
    speedInput = nil           -- Reference to the input box
}

-- Safely gets the game's current default walk speed
local function updateDefaultSpeed()
    pcall(function()
        local character = Players.LocalPlayer.Character
        if character and character:FindFirstChildOfClass("Humanoid") then
            WalkSpeedController.gameDefaultSpeed = character.Humanoid.WalkSpeed
            WalkSpeedController.lastSpeedCheck = os.time()
            
            -- Update placeholder to show current default
            if WalkSpeedController.speedInput then
                WalkSpeedController.speedInput:SetAttribute("PlaceholderText", 
                    "Default: "..WalkSpeedController.gameDefaultSpeed.." (0-âˆ)")
            end
        end
    end)
end

-- Safely applies the appropriate speed
local function applyWalkSpeed()
    local speedToApply = WalkSpeedController.usingCustomSpeed 
        and WalkSpeedController.currentSpeed 
        or WalkSpeedController.gameDefaultSpeed
    
    pcall(function()
        local character = Players.LocalPlayer.Character
        if character and character:FindFirstChildOfClass("Humanoid") then
            character.Humanoid.WalkSpeed = speedToApply
        end
    end)
end

-- Create the speed input control
WalkSpeedController.speedInput = PlayerTab:CreateInput({
    Name = "âˆ WalkSpeed control",
    PlaceholderText = "Default: "..(WalkSpeedController.gameDefaultSpeed or "16").." (0-âˆ)studs/s",
    ClearTextOnFocus = true,
    Numeric = true,
    Callback = function(input)
        if input == "" then
            -- Empty input means use game default
            WalkSpeedController.usingCustomSpeed = false
            WalkSpeedController.speedInput:Set("") -- Clear input
            updateDefaultSpeed() -- Refresh default immediately
        else
            -- Numeric input means use custom speed
            WalkSpeedController.usingCustomSpeed = true
            WalkSpeedController.currentSpeed = tonumber(input) or WalkSpeedController.gameDefaultSpeed
        end
        applyWalkSpeed()
    end
})

-- Background speed maintenance loop (120FPS)
task.spawn(function()
    while true do
        -- Periodically check if game changed default speed
        if not WalkSpeedController.usingCustomSpeed and 
           os.time() - WalkSpeedController.lastSpeedCheck > WalkSpeedController.checkInterval then
            updateDefaultSpeed()
        end
        
        applyWalkSpeed()
        task.wait(1/120) -- 120FPS
    end
end)

-- Handle character respawns
Players.LocalPlayer.CharacterAdded:Connect(function(character)
    character:WaitForChildOfClass("Humanoid")
    updateDefaultSpeed() -- Get fresh default speed
    applyWalkSpeed()     -- Reapply our settings
end)

-- Number pad support for quick input
UserInputService.InputBegan:Connect(function(input)
    if WalkSpeedController.speedInput and input.UserInputType == Enum.UserInputType.Keyboard then
        local key = input.KeyCode.Value
        if key >= 48 and key <= 57 then -- Number keys 0-9
            WalkSpeedController.speedInput:CaptureFocus()
        end
    end
end)

-- Initial setup
updateDefaultSpeed()

-- Jump Power Control with safety checks
PlayerTab:CreateSlider({
   Name = "Jump Power",
   Range = {50, 500},
   Increment = 5,
   Suffix = "power",
   CurrentValue = 50,
   Callback = function(Value)
      pcall(function()
         local character = game:GetService("Players").LocalPlayer.Character
         if character and character:FindFirstChildOfClass("Humanoid") then
            character.Humanoid.JumpPower = Value
         end
      end)
   end
})

